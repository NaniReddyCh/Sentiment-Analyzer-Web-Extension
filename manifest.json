{
  "manifest_version": 3,
  "name": "Webpage Feedback Extension",
  "version": "1.0",
  "description": "Collects feedback on the current webpage and performs sentiment analysis.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
